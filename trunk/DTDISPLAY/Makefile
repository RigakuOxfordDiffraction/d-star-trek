CC              = g++
OPTLEV          = -O3
DTREKINCLUDEDIR = ../DTTREK
DTREKOBJDIR     = ../DTTREK
DTREKLIBDIR     = ../DTTREK
INSTALLDIR      = ../../bin_irix
GINCLUDEDIR     = /usr/local/lib/g++-include
C++FLAGS        = $(OPTLEV) -I$(GINCLUDEDIR) -I$(DTREKINCLUDEDIR) -L$(DTREKLIBDIR) -DFUNCPROTO -DXTFUNCPROTO 
###-DUSE_OLD_ERROR_BITS
LIBS            = -ldtrek$(OSTYPE) -lXm -lXt -lX11 -lPW -lm

OFILES = main-cc.o UIComponent.o bxutils-cc.o callbacks-cc.o MainWindow.o \
         CXdisplay.o CXcolormap.o CXrubberband.o CXhelp.o CXcursor.o CXprop.o \
	 CUIom9.o CUIom8.o CUIom2.o CUIimageviewprops.o CUInumberField.o \
	 CUIreflnviewprops.o CUIimagefileSelect.o CUIreflnfileSelect.o \
	 CUIprintSelect.o

FILES = main-cc.cc UIComponent.cc bxutils-cc.cc callbacks-cc.cc MainWindow.cc \
	CXdisplay.cc CXcolormap.cc CXrubberband.cc CXhelp.cc CXcursor.cc \
	CXprop.cc \
	CUIom9.cc CUIom8.cc CUIom2.cc CUIimageviewprops.cc CUInumberField.cc \
	CUIreflnviewprops.cc CUIimagefileSelect.cc CUIreflnfileSelect.cc \
	CUIprintSelect.cc

ALL             = dtdisplay
RM  = rm -f

# User-specified options:

LIBOBJS		= CXdisplay.o CXcolormap.o CXrubberband.o CXhelp.o CXcursor.o \
		  CXprop.o

all: dtdisplay gifs

dtdisplay: $(OFILES)
	$(CC) $(C++FLAGS) $(OFILES) $(LIBS) -o dtdisplay

lib: libdtrekX$(OSTYPE).a

main-cc: $(OFILES)
	$(CC) $(C++FLAGS) $(OFILES) $(LIBS) -o $@

main-cc.o: main-cc.cc
	$(CC) $(C++FLAGS) -c main-cc.cc

jtest:  jtest.o CXprop.o
	$(CC) $^ $(C++FLAGS) $(LIBS)  -o $@ 

jtest.o: jtest.cc
	$(CC) $(C++FLAGS) -c jtest.cc

UIComponent.o: UIComponent.cc
	$(CC) $(C++FLAGS) -c UIComponent.cc

bxutils-cc.o: bxutils-cc.cc
	$(CC) $(C++FLAGS) -c bxutils-cc.cc

callbacks-cc.o: callbacks-cc.cc
	$(CC) $(C++FLAGS) -c callbacks-cc.cc

CUInumberField.o: CUInumberField.cc
	$(CC) $(C++FLAGS) -c CUInumberField.cc

CUIom8.o: CUIom8.cc
