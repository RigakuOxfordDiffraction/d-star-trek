install:	$(ALL)
		if [ ! -d $(INSTALLDIR) ];then mkdir $(INSTALLDIR);fi;
		for X in $(ALL); do \
			cp $$X $(INSTALLDIR); done

clean:
		$(RM) $(OFILES) *$(OSTYPE).a $(ALL)

cleanbin:
		$(RM) dtdisplay MainWindow.o


